//Remove Duplicates from Sorted List



public ListNode DeleteDuplicates(ListNode head) {
        ListNode l1 = head;
        if(head != null){
            ListNode l2 = l1.next;
        while(l2 != null){
            if(l1.val == l2.val){
                l2 = l2.next;
                l1.next = l2;
            }
            else{
                l2 = l2.next;
                l1 = l1.next;
            }
            
        }
        }
        
        
        return head;
    }
